<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx" minWidth="400" minHeight="400" 
		xmlns:utils="ro.calin.utils.*" xmlns:component="ro.calin.component.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("ro.calin.component.PictureViewer")]
	</fx:Metadata>
	
	<s:states>
		<s:State name="disabled"/>
		<s:State name="normal"/>
	</s:states>
	
	<component:ClippedImage id="picture1" width="{hostComponent.width}" height="{hostComponent.height}"/>
	<component:ClippedImage id="picture2" width="{hostComponent.width}" height="{hostComponent.height}"/>
</s:Skin>
